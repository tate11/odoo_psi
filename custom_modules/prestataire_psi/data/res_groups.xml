<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
         <record id="category_psi" model="ir.module.category">
            <field name="name">PSI</field>
        </record> 
        <record id="group_prestataire" model="res.groups">
            <field name="name">PSI/Prestataires</field>
            <field name="category_id" ref="category_psi"/>
            <field name="implied_ids" eval="[(4,[ref('base.user_root')])]"/>
        </record> 
    </data>
    
    <data noupdate="1">
        <record id="group_permanent" model="res.groups">
            <field name="name">PSI/Permanent</field>
            <field name="category_id" ref="base.module_category_hr_timesheet"/>
            <field name="implied_ids" eval="[(4,[ref('base.user_root')])]"/>
        </record> 
    </data>
    
</odoo>